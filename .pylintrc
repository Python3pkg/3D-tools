[MASTER]

# Pickle collected data for later comparisons.
persistent=no

# Use multiple processes to speed up Pylint.
jobs=0


[MESSAGES CONTROL]

# Disable the message, report, category or checker with the given id(s). You
# can either give multiple identifiers separated by comma (,) or put this
# option multiple times (only on the command line, not in the configuration
# file where it should appear only once).You can also use "--disable=all" to
# disable everything first and then reenable specific checks. For example, if
# you want to run only the similarities checker, you can use "--disable=all
# --enable=similarities". If you want to run only the classes checker, but have
# no Warning level messages displayed, use"--disable=all --enable=classes
# --disable=W"
disable=setslice-method,standarderror-builtin,reload-builtin,xrange-builtin,round-builtin,reduce-builtin,using-cmp-argument,parameter-unpacking,coerce-builtin,delslice-method,old-division,next-method-called,unpacking-in-except,indexing-exception,unichr-builtin,zip-builtin-not-iterating,suppressed-message,apply-builtin,oct-method,print-statement,hex-method,old-ne-operator,useless-suppression,raising-string,metaclass-assignment,import-star-module-level,no-absolute-import,coerce-method,map-builtin-not-iterating,input-builtin,range-builtin-not-iterating,execfile-builtin,backtick,cmp-method,dict-view-method,basestring-builtin,raw_input-builtin,unicode-builtin,getslice-method,long-suffix,filter-builtin-not-iterating,file-builtin,dict-iter-method,buffer-builtin,nonzero-method,long-builtin,old-octal-literal,intern-builtin,old-raise-syntax,cmp-builtin,invalid-name,locally-disabled


[REPORTS]

# Tells whether to display a full report or only the messages
reports=no


[FORMAT]

# Maximum number of characters on a single line.
max-line-length=80
